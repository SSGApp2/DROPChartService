    <!-- Chart Plugins Js -->
    <spring:url value="/resources/plugins/autosize/autosize.js" var="autosize"/>
    <script src="${autosize}" type="text/javascript"><!-- required for FF3 and Opera --></script>

    <spring:url value="/resources/plugins/momentjs/moment.js" var="moment"/>
    <script src="${moment}" type="text/javascript"><!-- required for FF3 and Opera --></script>

    <spring:url value="/resources/plugins/bootstrap-material-datetimepicker/js/bootstrap-material-datetimepicker.js" var="bootstrap_material_datetimepicker"/>
    <script src="${bootstrap_material_datetimepicker}" type="text/javascript"><!-- required for FF3 and Opera --></script>

    <spring:url value="/resources/plugins/bootstrap-datepicker/js/bootstrap-datepicker.js" var="bootstrap_datepicker"/>
    <script src="${bootstrap_datepicker}" type="text/javascript"><!-- required for FF3 and Opera --></script>




    <spring:url value="/resources/js/pages/charts/chartjs.js" var="chartjs"/>
    <script src="${chartjs}" type="text/javascript"><!-- required for FF3 and Opera --></script>



    <spring:url value="/resources/js/pages/forms/basic-form-elements.js" var="basic_form_elements"/>
    <script src="${basic_form_elements}" type="text/javascript"><!-- required for FF3 and Opera --></script>




</div>
